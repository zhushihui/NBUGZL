round(nbu_teacher_workload.d3,2) as d3_1,&#xD;
round(nbu_teacher_workload.d4,2) as d4_1,&#xD;
round(nbu_teacher_workload.d5,2) as d5_1,&#xD;
round(nbu_teacher_workload.d6,2) as d6_1&#xD;
