<model:Model xmlns:model="http://www.wisedu.com/emap/model/model">
  <actionModel caption="二次分配" displayRef="biz:JG_TW_CW_XX_COURSES" displayRefInclude="YEAR,TERM,JG0101ID,DWMC,KCID,KCMC,XM,WORKTYPE,D1_1,D2_1,D3_1,D4_1,D5_1,D6_1" displayRefPath="src/parammodel/JG_TW_CW_XX_COURSES.epm" displayRefPrj="nbugzl" name="ecfp" paramRef="biz:JG_TW_CW_XX_COURSES" paramRefInclude="CWID,YEAR,TERM,KCID,KCMC,D,D1,D2,D3,D4,D5,D6,JX0404ID,J,ZRN,TW_ID,YEAR_1,TERM_1,JG0101ID,CWID_1,GKDW,D_1,D1_1,D2_1,D3_1,D4_1,D5_1,D6_1,JX0404ID_1,XX0301ID,DWMC,JG0101ID_1,XM,XX0301ID_1,GKDW_1,XX0301ID_2,WORKTYPE" paramRefPath="src/parammodel/JG_TW_CW_XX_COURSES.epm" paramRefPrj="nbugzl" saveLog="true" totalCountType="AUTO" type="QUERY">
    <sql>select nbu_course_workload.YEAR,nbu_course_workload.TERM,&#xD;
nbu_teacher_workload.jg0101id,nbu_courses.worktype,&#xD;
xx0301.dwmc,nbu_courses.kcid,&#xD;
